			dragging2 = true
			dragStart2 = input.Position
			startPos2 = OptionSpage.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging2 = false
				end
			end)
		end
	end)
	
	OptionSpage.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput2 = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput2 and dragging2 then
			update2(input)
		end
	end)
	
	--Synapse X Button Controller 2
	
	local function update4(input)
		local delta = input.Position - dragStart4
		SynapseXButton.Position = UDim2.new(startPos4.X.Scale, startPos4.X.Offset + delta.X, startPos4.Y.Scale, startPos4.Y.Offset + delta.Y)
	end
	
	SynapseXButton.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging4 = true
			dragStart4 = input.Position
			startPos4 = SynapseXButton.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging4 = false
				end
			end)
		end
	end)
	
	SynapseXButton.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput4 = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput4 and dragging4 then
			update4(input)
		end
	end)
	
	--Synapse X Script Hub Controller
	
	ScriptHubButton.MouseButton1Click:Connect(function()
		if ScriptHub.Visible == true then
			ScriptHub.Visible = false
		elseif ScriptHub.Visible == false then
			ScriptHub.Visible = true
		end
	end)
	
	--Synapse X Script Hub Controller 2
	
	local function update5(input)
		local delta = input.Position - dragStart5
		ScriptHub.Position = UDim2.new(startPos5.X.Scale, startPos5.X.Offset + delta.X, startPos5.Y.Scale, startPos5.Y.Offset + delta.Y)
	end
	
	ScriptHub.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging5 = true
			dragStart5 = input.Position
			startPos5 = ScriptHub.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging5 = false
				end
			end)
		end
	end)
	
	ScriptHub.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput5 = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput5 and dragging5 then
			update5(input)
		end
	end)
	
	--Synapse X Script Hub Controller 4
	
	CloseScriptHub.MouseButton1Click:Connect(function()
		ScriptSelected = "None"
		ScriptHub.Visible = false
	end)
	
	--Synapse X Script Hub Controller 5
	
	ExecuteScriptHub.MouseButton1Click:Connect(function()
		if Injected == true then
			if ScriptSelected == "None" then
	
			end
			if ScriptSelected == "Dex" then
				loadstring(game:HttpGet("https://pastebin.com/raw/J8AX35Fg"))()
			end
			if ScriptSelected == "Spy" then
				loadstring(game:HttpGet"https://gitlab.com/te4224/Scripts/-/raw/main/Universal%20Aimbot%20N%20Esp/v2/main.lua")()
			end
			if ScriptSelected == "Esp" then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua"))()
			end
			if ScriptSelected == "Backdoor" then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/iK4oS/backdoor.exe/master/source.lua"))()
			end
		end
		if Injected == false then
			ExecuteScriptHub.Text = "Not Attached!"
			wait(1)
			ExecuteScriptHub.Text = "Execute"
		end
	end)
	
	--Synapse X Attach Controller
	
	Attach.MouseButton1Click:Connect(function()
		if Injected == true then
			Title.Text = "Synapse X - "..SynapseXVerison.." (Already injected!)"
			wait(1)
			Title.Text = "Synapse X - "..SynapseXVerison.." (Beta)"
		end
		if Injected == false then
			Title.Text = "Synapse X - "..SynapseXVerison.." (Checking...)"
			wait(0.1)
			Title.Text = "Synapse X - "..SynapseXVerison.." (Injecting...)"
			wait(1.5)
			Title.Text = "Synapse X - "..SynapseXVerison.." (Checking Whitelist...)"
			wait(0.8)
			Title.Text = "Synapse X - "..SynapseXVerison.." (Scanning...)"
			wait(0.3)
			Title.Text = "Synapse X - "..SynapseXVerison.." (Ready!)"
			wait(0.5)
			Title.Text = "Synapse X - "..SynapseXVerison..""
			Injected = true
		end
	end)
	
	--Synapse Execute Controller
	
	Execute.MouseButton1Click:Connect(function()
		if Injected == true then
			loadstring(Source.Text)()
		end
		if Injected == false then
			Title.Text = "Synapse X - "..SynapseXVerison.." (Not injected! Press inject!)"
		end
	end)
	
	--Synapse X Clear Controller
	
	Clear.MouseButton1Click:Connect(function()
		Source.Text = ""
	end)
	
	--Synapse X Script Hub Controller 7
	
	while wait() do
		if ScriptHub.Visible == true then
			if ScriptSelected == "None" then
				DescriptionText.Text = ""
			end
			if ScriptSelected == "Dex" then
				DescriptionText.Text = "A version of the popular Dex explorer with patches specifically for Synapse X."
			end
			if ScriptSelected == "Esp" then
				DescriptionText.Text = "ESP Made by ic3w0lf using the Drawing API."
			end
			if ScriptSelected == "Spy" then
				DescriptionText.Text = "Aimbot/Esp Credits in script."
			end
			if ScriptSelected == "Backdoor" then
				DescriptionText.Text = "backdoor.exe for scan place to FE access (Only place ban possible)."
			end
		end
	end
end
coroutine.wrap(TFKNA_fake_script)()
